    <Label text="Inicio de sesión" />
    <TextField fx:id="usernameField" prefHeight="25.0" prefWidth="679.0" promptText="nombre" />
    <PasswordField fx:id="passwordField" promptText="Contraseña" />
    <Button fx:id="loginButton" text="Iniciar sesión" onAction="#loginUser" />
</VBox>
